{
  "name": "frontend-test-work",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint ./src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 && stylelint 'src/assets/scss/**/*.scss'",
    "lint:fix": "eslint ./src --fix --ext ts,tsx --report-unused-disable-directives --max-warnings 0 && prettier --write './src/**/*.{ts,tsx}' && stylelint 'src/assets/scss/**/*.scss' --fix",
    "preview": "vite preview"
  },
  "dependencies": {
    "autoprefixer": "^10.4.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "sass": "^1.66.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "prettier": "^3.0.3",
    "stylelint": "^15.10.3",
    "stylelint-config-standard-scss": "^11.0.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
